ustify-content: center;\r\n  width: 200px;\r\n  height: 50px;\r\n  border: 3px #34508b solid;\r\n  background: rgb(189, 44, 59);\r\n  background: linear-gradient(\r\n    90deg,\r\n    rgba(189, 44, 59, 1) 0%,\r\n    rgba(9, 121, 91, 1) 96%,\r\n    rgba(0, 212, 255, 1) 100%\r\n  );\r\n  margin-top: 20px;\r\n  color: white;\r\n  font-family: Arial, Helvetica, sans-serif;\r\n  font-size: 2rem;\r\n  margin-bottom: 20px;\r\n}\r\n#start-button {\r\n  cursor: pointer;\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n  height: 27px;\r\n  border-radius: 10px;\r\n  color: white;\r\n  background: rgb(19, 16, 17);\r\n  background: linear-gradient(\r\n    90deg,\r\n    rgba(19, 16, 17, 1) 0%,\r\n    rgba(54, 85, 77, 1) 96%,\r\n    rgba(0, 212, 255, 1) 100%\r\n  );\r\n}\r\n#start-button:hover {\r\n  background: rgb(189, 44, 59);\r\n  background: linear-gradient(\r\n    90deg,\r\n    rgba(189, 44, 59, 1) 0%,\r\n    rgba(9, 121, 91, 1) 96%,\r\n    rgba(0, 212, 255, 1) 100%\r\n  );\r\n  border: 2px solid red;\r\n}\r\n.hide {\r\n  display: none;\r\n}\r\n\r\n.square {\r\n  display: block;\r\n}\r\n\r\n.blank {\r\n  background: black;\r\n}\r\n\r\n.wall {\r\n  border: 1px solid blue;\r\n}\r\n\r\n.dot {\r\n  background: #f7c400;\r\n  border: 8px solid black;\r\n  border-radius: 50%;\r\n}\r\n@keyframes mycolor {\r\n  from {\r\n    background-color: red;\r\n  }\r\n  to {\r\n    background-color: blue;\r\n  }\r\n}\r\n.pill {\r\n  background: white;\r\n  border: 5px solid black;\r\n  border-radius: 50%;\r\n  animation: mycolor 1s infinite;\r\n}\r\n\r\n.ghost {\r\n  position: relative;\r\n  border-radius: 10px 10px 2px 2px;\r\n  border: none;\r\n}\r\n\r\n.ghost::before {\r\n  content: \"\";\r\n  display: block;\r\n  position: absolute;\r\n  width: 3px;\r\n  height: 3px;\r\n  background: black;\r\n  border-radius: 50%;\r\n  left: 3px;\r\n  top: 5px;\r\n  border: 2px solid white;\r\n}\r\n\r\n.ghost::after {\r\n  content: \"\";\r\n  display: block;\r\n  position: absolute;\r\n  width: 3px;\r\n  height: 3px;\r\n  background: black;\r\n  border-radius: 50%;\r\n  right: 3px;\r\n  top: 5px;\r\n  border: 2px solid white;\r\n}\r\n\r\n.scared {\r\n  background: blue !important;\r\n}\r\n\r\n.blinky {\r\n  background: red;\r\n}\r\n\r\n.pinky {\r\n  background: pink;\r\n}\r\n\r\n.inky {\r\n  background: cyan;\r\n}\r\n\r\n.clyde {\r\n  background: orange;\r\n}\r\n.square.blank.pacman,\r\n.square.dot.pacman {\r\n  width: 20px !important ;\r\n  height: 20px !important;\r\n}\r\n.pacman {\r\n  background: yellow;\r\n  border: none;\r\n\r\n  border-radius: 50%;\r\n  clip-path: polygon(\r\n    0 0,\r\n    20px 0,\r\n    20px -5px,\r\n    8px 10px,\r\n    20px 25px,\r\n    20px 20px,\r\n    0 20px\r\n  );\r\n  animation: chew 0.2s linear forwards infinite;\r\n}\r\n\r\n@keyframes chew {\r\n  50% {\r\n    clip-path: polygon(\r\n      0 0,\r\n      20px 0,\r\n      20px 10px,\r\n      8px 10px,\r\n      20px 10px,\r\n      20px 20px,\r\n      0 20px\r\n    );\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"4e70aae9a9f7a01b28a99d8134dd924b","cacheData":{"env":{}}}